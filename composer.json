    "type": "library",
    "description": "A wrapper for the League of Legends API.",
    "version": "1.0.0",
    "keywords": ["League", "legends", "wrap", "api", "facade", "proxy"],
    "homepage": "https://github.com/armonkolaei/leaguewrap",
    "license": "MIT",
    "authors": [
        {
            "name": "Armon Kolaei",
            "email": "armonkolaei@gmail.com",
            "homepage": "http://kolaeigroup.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8.0",
        "mockery/mockery": "~0.9.0",
        "satooshi/php-coveralls": "~0.6.0"
    },
    "autoload": {
        "psr-0": { 
